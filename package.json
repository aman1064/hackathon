  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,jsx}": [
        "eslint --no-ignore --fix",
        "git add --force"
      ],
      "*.{json,md}": [
        "prettier --write",
        "git add --force"
      ],
      "*.{css,less,styl,scss,sass}": [
        "stylelint --fix",
        "git add --force"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build-sw": "node ./src/sw-build.js",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "build": "node scripts/build.js --static && npm run build-sw && npm run clean-cra-sw",
    "test": "node scripts/test.js",
    "build:prod": "node scripts/build.js --static --release"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.js"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@redux-saga/testing-utils": "^1.0.0",
    "clean-css": "^4.2.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "extract-css-chunks-webpack-plugin": "^3.3.2",
    "highland": "^2.13.0",
    "html-minifier": "^3.5.21",
    "mimic": "^2.0.9",
    "minimalcss": "^0.8.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.15.3",
    "puppeteer": "^1.20.0",
    "react-test-renderer": "^16.7.0",
    "redux-devtools-extension": "^2.13.7",
    "serve-static": "^1.13.2",
    "sourcemapped-stacktrace-node": "^2.1.8",
    "svg-sprite-loader": "^4.2.1",
    "svgo-loader": "^2.2.1",
    "tv4": "^1.3.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "workbox-build": "^4.0.0",
    "worker-loader": "^2.0.0",
    "zip-webpack-plugin": "^3.0.0",
    "react-dev-utils": "^7.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-config-prettier": "^6.4.0",
